<?xml version="1.0" encoding="ISO-8859-1"?>
<testdefinition version="1.0">
  <suite name="signon-plugins-tests" domain="Accounts and SSO">
<set description="signon-plugins-tests:signon-plugins-tests"
name="./signon-plugins-tests" feature="SSO plugin framework">
      <description>signon-plugins-tests:signon-plugins-tests</description>
      <case name="signon-plugins-tests-pluginsPool" type="Integration" level="Component">
        <description>signon-plugins-tests:signon-plugins-tests:pluginsPool</description>
        <step>/usr/bin/signon-plugins-tests pluginsPool</step>
      </case>
      <case name="signon-plugins-tests-loadPlugins" type="Integration" level="Component">
        <description>signon-plugins-tests:signon-plugins-tests:loadPlugins</description>
        <step>/usr/bin/signon-plugins-tests loadPlugins</step>
      </case>
      <case name="signon-plugins-tests-unloadPlugins" type="Integration" level="Component">
        <description>signon-plugins-tests:signon-plugins-tests:unloadPlugins</description>
        <step>/usr/bin/signon-plugins-tests unloadPlugins</step>
      </case>
      <case name="signon-plugins-tests-checkPlugins" type="Integration" level="Component">
        <description>signon-plugins-tests:signon-plugins-tests:checkPlugins</description>
        <step>/usr/bin/signon-plugins-tests checkPlugins</step>
      </case>
      <environments>
        <scratchbox>true</scratchbox>
        <hardware>true</hardware>
      </environments>
    </set>
  </suite>
</testdefinition>
